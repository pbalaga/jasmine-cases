    "Greg Burghardt <greg_burghardt@yahoo.com>",
    "Daniel Koza <dakolech@gmail.com> (https://github.com/dakolech)",
    "Pawel Balaga <pbalaga.code@gmail.com> (https://github.com/pbalaga)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pbalaga/jasmine-cases/issues"
  },
  "homepage": "https://github.com/pbalaga/jasmine-cases",
  "devDependencies": {
    "@types/jasmine": "^2.5.43",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-jasmine-html-reporter": "^0.2.2",
