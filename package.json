    "test-travis": "./node_modules/.bin/karma start --single-run --browsers=Chrome_travis_ci,Firefox,PhantomJS",
    "build": "./node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pbalaga/jasmine-cases.git"
  },
